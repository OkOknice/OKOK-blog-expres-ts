{
  "verbose": false,
  "debug": false,
  "exec": "ts-node  ./bin/serve.ts",
  "ignore": [
    "mochawesome-report",
    "node_modules",
    "./test",
    "**/*.d.ts",
    "*.test.ts",
    "*.spec.ts",
    "fixtures/*",
    "test/**/*",
    "docs/*"
  ],
  "events": {
    "restart": ""
  },
  "watch": [
    "./app",
    "./app.ts",
    "./db/*",
    "./services/*",
    "./routes/*",
    "./utils/*"
  ],
  "ext": "ts",
  "inspect": true
}